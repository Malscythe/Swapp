<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="Swapp">
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/swappicon"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Swapp">
    <activity
        android:name="com.example.Swapp.signup"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.popup"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.MoreInfo"
        android:exported="true" />


    <activity
        android:name="com.example.Swapp.OfferSecondActivity"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.offerItem"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.UserHomepage"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.OfferMainAcitvity"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.Categories"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.chat.Chat"
        android:windowSoftInputMode="adjustResize"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.ItemSwipe"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.forgotpassword"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.Messages"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.postforgotpassword"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.PostItem"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.OfferMoreInfo"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.deliverytrack"
        android:exported="true" />
    <activity
        android:name="com.example.Swapp.currentLoc"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.FileMaintenance"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
        android:exported="true" />

    <!--
     TODO: Before you run your application, you need a Google Maps API key.

     To get one, follow the directions here:

        https://developers.google.com/maps/documentation/android-sdk/get-api-key

     Once you have your API key  (it starts with  "AIza"),  define a new property in your
     project's local.properties file (e.g. MAPS_API_KEY=Aiza...), and replace the
     "YOUR_API_KEY" string in this file with "${MAPS_API_KEY}".
 -->
    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="@string/googleApi" />

    <activity
        android:name="com.example.Swapp.AdminHomepage"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.login"
        android:exported="true">


        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <meta-data
        android:name="preloaded_fonts"
        android:resource="@array/preloaded_fonts" />
</application>

</manifest>
