<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="Swapp">
<uses-permission android:name="android.permission.INTERNET" />
<application
    android:allowBackup="true"
    android:icon="@mipmap/swappicon"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Swapp">
    <activity
        android:name="com.example.Swapp.signup"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.popup"
        android:theme="@style/Theme.AppCompat.CustomTheme"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.MoreInfo"
        android:theme="@style/Theme.AppCompat.CustomTheme"
        android:exported="true" />
    

    <activity
        android:name="com.example.Swapp.UserHomepage"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.Categories"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.ItemSwipe"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.forgotpassword"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.postforgotpassword"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.PostItem"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.FileMaintenance"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.AdminHomepage"
        android:exported="true" />

    <activity
        android:name="com.example.Swapp.login"
        android:exported="true">

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <meta-data
        android:name="preloaded_fonts"
        android:resource="@array/preloaded_fonts" />
</application>

</manifest>